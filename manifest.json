{

  "description": "A productivity addon",
  "manifest_version": 2,
  "name": "ductivityPRO",
  "version": "1.0",
  "homepage_url": "https://github.com/sauronthemighty/pro-ductivity",
  "icons": {
    "48": "icons/icon48.png",
    "84": "icons/icon.png",
    "96": "icons/icon96.png"
  },

  "permissions": [
    "activeTab",
    "alarms",
    "notifications"
  ],

  "browser_action": {
    "default_icon": {
      "48": "icons/icon48.png",
      "64": "icons/icon.png",
      "96": "icons/icon96.png"
    },
    "default_title": "ProDuctivity",
    "default_popup": "popup/pro.html"
  },

  "background": {
    "scripts": ["timer.js"]
  }

}